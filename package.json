{
  "name": "@fav/text.constant-case",
  "version": "1.0.2",
  "description": "Converts a string into constant case.",
  "main": "index.js",
  "files": [
    "index.js",
    "web/"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "web:build": "npm run web:build-src && npm run web:build-test",
    "web:build-src": "browserify index.js --standalone fav.text.constant-case -o web/fav.text.constant-case.js && cd web && uglifyjs fav.text.constant-case.js --compress --mangle -o fav.text.constant-case.min.js --source-map url=\"'fav.text.constant-case.min.js.map'\"",
    "web:build-test": "node tool/mktest.js",
    "chrome:install": "npm i --no-save mocha-chrome",
    "chrome:test": "mocha-chrome test/web/browser-test.html",
    "build": "npm run lint && npm run coverage && npm run web:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/fav-text.constant-case.git"
  },
  "keywords": [
    "constant",
    "case",
    "string",
    "text",
    "fav"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/fav-text.constant-case/issues"
  },
  "homepage": "https://github.com/sttk/fav-text.constant-case#readme",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "benchmark-tester": "^0.3.0",
    "browserify": "^16.1.0",
    "chai": "^3.5.0",
    "constant-case": "^2.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.16.0",
    "lodash": "^4.17.11",
    "mocha": "^3.5.3",
    "nyc": "^14.1.0",
    "platform": "^1.3.5",
    "uglify-js": "^3.1.3"
  },
  "dependencies": {
    "@fav/text.camel-case": "^1.0.1"
  }
}
